{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000",
        "Protocols": "Http1"
      },
      "Https": {
        "Url": "https://*:5001"
      },
      //"HttpsDefaultCert": {
      //  "Url": "https://*:5001",
      //  "Protocols": "Http1"
      //},
      //"HttpsInlineCertFile": {
      //  "Url": "https://*:5001",
